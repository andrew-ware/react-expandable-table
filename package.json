{
  "name": "aw-table",
  "version": "1.0.0",
  "description": "Lightweight, flexible react table component, with only one dependency.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run styleguide:clean && npm run styleguide:build && npm run docgen && npm run propsToHtml webpack",
    "clean": "rimraf dist/",
    "docgen": "react-docgen src/Table.jsx -o docgen/Table.props.json --pretty",
    "lint": "eslint --ext .js,.jsx .",
    "prepublish": "npm run build",
    "propsToHtml": "node scripts/propsToHtml",
    "start": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "styleguide:clean": "rimraf styleguide/",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/lib/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "es6-promisify": "^5.0.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^21.2.1",
    "node-sass": "^4.7.2",
    "prettier": "^1.9.2",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react-docgen": "^2.20.0",
    "react-styleguidist": "^6.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.3",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
